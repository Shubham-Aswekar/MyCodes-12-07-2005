#include<iostream>
using namespace std;
class myClass{
    private:
    float length,breadth,area;
    
    public:
    myClass(float l,float b){
        length = l;
        breadth = b;
    }
    void result(){
        area = length*breadth;
        cout<<"area is "<<area<<endl;
    }
};
int main(){
    myClass obj1(31.7,62.8);
    obj1.result();
    return 0;
}

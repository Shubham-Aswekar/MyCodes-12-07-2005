#include<iostream>
using namespace std;
int main(){
    int num1,num2,num3;
    cout<<"Enter Number 1: ";
    cin>>num1;
    cout<<"Enter Number 2: ";
    cin>>num2;
    cout<<"Enter Number 3: ";
    cin>>num3;
    cout<<endl;
    
    if(num1 > num2 && num1 > num3){
        cout<<num1<<" is Largest";
    }
    else if(num2 > num3){
        cout<<num2<<" is Largest";
    }
    else{
        cout<<num3<<" is Largest";
    }
}
